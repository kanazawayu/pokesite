.result
  .result__title
    ダメージ計算
  .result__calc
    .result__calc__my
      %span
        自分のこうげき:
      %span#myares
        ーーー
      %span
        自分のとくこう:
      %span#mycres
        ーーー
    .result__calc__your
      %span
        相手のHP:
      %span#youhres
        ーーー
      %span
        相手のぼうぎょ:
      %span#youbres
        ーーー
      %span
        相手のとくぼう:
      %span#youdres
        ーーー
    .result__calc__tec
      %span
        わざ
      %span
        ぶんるい
      %select{id: 'aorc', oninput: 'damageclac()'}
        %option{value: '0'}
          物理
        %option{value: '1'}
          特殊
      %span
        いりょく
      %input{id: 'power',type: 'number', oninput: 'damageclac()'}
      %span
        タイプ一致補正
      %select{id: 'match', oninput: 'damageclac()'}
        %option{value: '1.5'}
          あり
        %option{value: '1'}
          なし
      %span
        タイプ相性
      %select{id: 'comp', oninput: 'damageclac()'}
        %option{value: '4'}
          ばつぐん(4倍)
        %option{value: '2'}
          ばつぐん(2倍)
        %option{value: '1', selected: 'selected'}
          等倍
        %option{value: '0.5'}
          いまひとつ(1/2)
        %option{value: '0.25'}
          いまひとつ(1/4)
        %option{value: '0'}
          こうかがない
    .result__calc__env
      %span
        天候補正(攻撃側)
      %select{id: 'myweat', oninput: 'damageclac()'}
        %option{value: '1.5'}
          あり(1.5倍)
        %option{value: '1', selected: 'selected'}
          なし
      %span
        フィールド補正(攻撃側)
      %select{id: 'myfield', oninput: 'damageclac()'}
        %option{value: '1.3'}
          あり(1.3倍)
        %option{value: '1', selected: 'selected'}
          なし
      %span
        天候補正(防御側)
      %select{id: 'youweat', oninput: 'damageclac()'}
        %option{value: '0.5'}
          あり(0.5倍)
        %option{value: '1', selected: 'selected'}
          なし
      %span
        フィールド補正(防御側)
      %select{id: 'youfield', oninput: 'damageclac()'}
        %option{value: '0.5'}
          あり(0.5倍)
        %option{value: '1', selected: 'selected'}
          なし
    .result__calc__stats
      %span
        攻撃側状態
      %select{id: 'mystats', oninput: 'damageclac()'}
        %option{value: '1'}
          なし
        %option{value: '0.5'}
          やけど
      %span
        防御側状態
      %select{id: 'youstats', oninput: 'damageclac()'}
        %option{value: '1'}
          なし
        %option{value: '0.5'}
          壁
      %span
